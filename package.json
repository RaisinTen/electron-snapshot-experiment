{
  "name": "electron-snapshot-experiment",
  "version": "1.0.0",
  "private": true,
  "main": "main.js",
  "repository": "git@github.com:RaisinTen/electron-snapshot-experiment.git",
  "author": "Darshan Sen <raisinten@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "electron main.js",
    "use-snapshot": "rm -rf cache *.bin && yarn create-snapshot && yarn copy-snapshot && yarn read-fuses",
    "create-snapshot": "node tools/create-v8-snapshot.js",
    "copy-snapshot": "node tools/copy-v8-snapshot.js",
    "reset-snapshot": "node tools/reset-v8-snapshot.js && yarn read-fuses",
    "read-fuses": "electron-fuses read --app node_modules/electron/dist/Electron.app",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "async": "1.5.2",
    "circular-json": "0.3.0",
    "electron": "28.3.1",
    "electron-json-storage": "4.5.0",
    "i18next": "23.5.1",
    "lodash": "4.17.21",
    "nedb": "1.8.0",
    "perftrace": "^1.2.1",
    "semver": "5.5.1",
    "shelljs": "0.8.4",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "@electron/fuses": "^1.8.0",
    "electron-link": "^0.6.0",
    "electron-mksnapshot": "28.3.1",
    "patch-package": "^8.0.0"
  }
}
